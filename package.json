    "term",
    "console",
    "ascii",
    "unicode",
    "blessed",
    "robo3t",
    "vim",
    "tmux",
    "ranger"
  ],
  "author": {
    "name": "infely",
    "email": "infely@gmail.com",
    "url": "https://github.com/infely"
  },
  "license": "MIT",
  "dependencies": {
    "blessed": "^0.1.81",
    "bson": "^4.2.2",
    "commander": "^7.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongodb": "3.6.3",
    "tunnel-ssh": "^4.1.4"
  },
  "devDependencies": {
    "pkg": "^4.4.9",
    "shebang-loader": "^0.0.1",
