  "description": "Command-line cross-platform MongoDB management TUI tool",
  "repository": "infely/mngr",
  "homepage": "https://github.com/infely/mngr",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack && cp {package.json,README.md} dist/npm",
